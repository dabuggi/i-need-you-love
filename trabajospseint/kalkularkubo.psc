Proceso kalkularkubo
	
	definir numero Como Real;
	definir cuadrado Como Real;
	definir cubo Como Real;
	
	escribir " escribe numero";
	leer numero;
	
	cuadrado<-numero*numero;
	cubo <- numero*numero*numero;
	escribir " el cuadrado de ",numero, "es:",cuadrado;
	escribir"el cubode",numero,"es:",cubo;
	
FinProceso
